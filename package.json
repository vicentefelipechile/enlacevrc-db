{
	"name": "enlacevrc-db",
	"version": "0.0.0",
	"private": true,
	"scripts": {
		"deploy": "wrangler deploy",
		"dev": "wrangler dev",
		"start": "wrangler dev",
		"test": "vitest",
		"cf-typegen": "wrangler types",
		"db:clear:local": "wrangler d1 execute enlacevrc-db --file=./db/clear_tables.sql",
		"db:clear:prod": "wrangler d1 execute enlacevrc-db --file=./db/clear_tables.sql --remote",
		"db:create:local": "wrangler d1 execute enlacevrc-db --file=./db/schema.sql",
		"db:create:prod": "wrangler d1 execute enlacevrc-db --file=./db/schema.sql --remote",
		"db:populate:local": "wrangler d1 execute enlacevrc-db --file=./db/poblate.sql",
		"db:populate:prod": "wrangler d1 execute enlacevrc-db --file=./db/poblate.sql --remote",
		"db:test:local": "wrangler d1 execute enlacevrc-db --file=./db/test.sql",
		"db:test:prod": "wrangler d1 execute enlacevrc-db --file=./db/test.sql --remote",
		"db:reload:local": "npm run db:clear:local && npm run db:create:local && npm run db:populate:local",
		"db:reload:prod": "npm run db:clear:prod && npm run db:create:prod && npm run db:populate:prod",
		"generate-pk": "node src/generate-key.js"
	},
	"devDependencies": {
		"@cloudflare/vitest-pool-workers": "^0.8.19",
		"typescript": "^5.5.2",
		"vitest": "~3.2.0",
		"wrangler": "^4.40.2"
	},
	"license": "MIT"
}